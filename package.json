{
  "name": "vehicle_registration_jen",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.2.5",
    "axios": "^0.19.2",
    "moment": "^2.26.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-moment": "^0.9.7",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.1.1",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier"
    ],
    "plugins": [
      "jsx-a11y"
    ],
    "settings": {
      "react": {
        "version": "detect"
      },
      "import/resolver": {
        "webpack": {
          "config": "webpack/propertytax.webpack.config.js"
        }
      }
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "allowImportExportEverywhere": true,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "node": true,
      "jest": true
    },
    "globals": {
      "window": true,
      "document": true,
      "FormData": true,
      "fetch": false,
      "localStorage": true,
      "sessionStorage": true
    },
    "rules": {
      "strict": [
        "error",
        "global"
      ],
      "no-param-reassign": 0,
      "object-shorthand": 0,
      "prefer-destructuring": 0,
      "class-methods-use-this": 0,
      "prefer-template": 0,
      "no-unused-vars": 0,
      "indent": [
        "error",
        2
      ],
      "no-console": 0,
      "linebreak-style": 0,
      "import/no-cycle": 0,
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "to"
          ],
          "aspects": [
            "noHref",
            "invalidHref",
            "preferButton"
          ]
        }
      ],
      "max-len": [
        1,
        130,
        2
      ],
      "newline-per-chained-call": [
        "error",
        {
          "ignoreChainWithDepth": 2
        }
      ],
      "react/destructuring-assignment": [
        0,
        "error",
        "always"
      ],
      "react/forbid-prop-types": [
        2,
        {
          "forbid": [
            "any"
          ]
        }
      ],
      "react/jsx-curly-spacing": [
        0
      ],
      "react/jsx-filename-extension": [
        2,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react-hooks/rules-of-hooks": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "prettier": "^2.2.1"
  }
}
